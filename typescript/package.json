{
  "name": "webgl-ann-sample-ts",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "bundle": "esbuild src/index.ts --bundle --sourcemap --minify --outfile=./war/webgl_ann_sample/app.bundle.js",
    "bundle:remote": "esbuild src/remoteEntry.ts --bundle --sourcemap --minify --outfile=./war/js/index.js",
    "bundle:local": "esbuild src/index.ts --bundle --sourcemap --minify --outfile=./war/webgl_ann_sample/app.bundle.js",
    "bundle:remote:local": "esbuild src/remoteEntry.ts --bundle --sourcemap --minify --outfile=./war/js/index.js",
    "dev": "esbuild src/index.ts --bundle --sourcemap --outfile=./war/webgl_ann_sample/app.bundle.js --servedir=./war --watch",
    "start": "npm run bundle:local && npm run bundle:remote:local && npx serve ./war -p 8000",
    "clean": "rimraf ./war/webgl_ann_sample/app.bundle.js ./war/js/index.js ./war/app.bundle.js dist"
  },
  "devDependencies": {
    "ts-jest": "29.1.0",
    "typescript": "5.2.2",
    "jest": "29.6.1",
    "@types/jest": "29.5.4",
    "esbuild": "0.19.0",
    "serve": "14.2.0",
    "rimraf": "5.0.0"
  }
}
